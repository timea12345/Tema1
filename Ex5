public class Ex5 {
    public static void main(String[] args){
        char charValue = 'a';
        int intValue = 150;

        //stiu ca puteam sa scriu direct 'a'> insa atunci era in zadar declararea variabilei de tip char
        if (charValue>intValue) {
            System.out.println("MAI MARE");
        }
            else if (charValue==intValue) {
                System.out.println("EGAL");
            } else System.out.println("MAI MIC");
    }
}
